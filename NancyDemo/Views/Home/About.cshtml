@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<NancyDemo.Models.SiteInformation>
@{
    Layout = "Views/Shared/_Layout.cshtml";
}
<div class="container">

    <h1>About</h1>
    <div class="row">
        <div class="col-md-6">@Model.Name</div>
        <div class="col-md-6">@Model.Owner</div>
    </div>
    <div class="row">
        <div class="col-md-6">@Model.Description</div>
        <div class="col-md-6">@Model.CreationDate</div>
    </div>
</div>


